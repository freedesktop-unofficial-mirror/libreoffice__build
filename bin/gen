#!/usr/bin/perl -w

$dest = shift @ARGV || die "no dest";

for $a (@ARGV) {
    $a =~ /~$/ && next;
    $a =~ /\#$/ && next;
    my $patch = lc ($a);
    $patch =~ s/\//-/g;
    $patch =~ s/\./-/g;
    $patch .= '.diff';
    print "regenerate $patch from $a\n";
    `diff --new-file -u /dev/null $a > $dest/$patch`;
}
