gawk ' BEGIN        { wroteindexentry=0; }
       /^<\/ul>/    {
                      if (wroteindexentry == 0)
                        {
                          line=$0;
                          while (getline <"'$indexentry'")
                            print;
                          close ("'$indexentry'");
                          print line;
                          wroteindexentry=1;
                          next;
                        }
                    }
       /^<\/body>/  {
                      line=$0;
                      while (getline <"'$body'")
                        print;
                      close ("'$body'");
                      print line;
                      next;
                    }
                    { print; }
'
