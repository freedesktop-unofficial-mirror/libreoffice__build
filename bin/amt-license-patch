#!/bin/sh

indexentry=`mktemp`
body=`mktemp`

trap "rm $indexentry $body" EXIT

cat >$indexentry <<'EOF'
  <li><a href="#mozTocId123456">Agfa Monotype Corporation </a>
    <ul>
      <li><a href="#mozTocId123429">Font Software</a>
      </li>
    </ul>
  </li>
EOF

cat >$body  <<'EOF'
