#!/usr/bin/env perl

my $num_funcs = 50;

sub generate_class($$)
{
    my $fh = shift;
    my $name = shift;

    print $fh "class $name {\n";
    print $fh "    $name(); /* key method */\n";
    for (my $i = 0; $i < $num_funcs; $i++) {
	print $fh "virtual int myTestFunction$i (int i)\n";
	print $fh "{ return $i; }\n";
    }
    print $fh "};\n";
    print $fh "$name" . "::" . "$name() { }\n";
}

for (my $i = 0; $i < 200; $i++)
{
    generate_class(STDOUT, "Test$i");
}

