+class KDEData : public SalData
+{
+public:
+    KDEData() {}
+    virtual ~KDEData();
+    
+    virtual void Init();
+    virtual void initNWF();
+    virtual void deInitNWF();
+};
+
+class KDESalFrame : public X11SalFrame
+{
+    static const int nMaxGraphics = 2;
+
+    struct GraphicsHolder
+    {
+        X11SalGraphics*		pGraphics;
+        bool				bInUse;
+        GraphicsHolder() 
+                : pGraphics( NULL ),
+                  bInUse( false )
+        {}
