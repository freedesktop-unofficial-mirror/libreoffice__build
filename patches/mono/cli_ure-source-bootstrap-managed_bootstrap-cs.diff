+namespace unoidl.com.sun.star.uno
+{
+    public interface XComponentContext
+    {
+    }
+}
+
+namespace uno.util
+{
+
+using System;
+using System.Collections;
+using System.Runtime.InteropServices;
+
+public class Bootstrap
+{
+    private Bootstrap() {}
+
+    public static unoidl.com.sun.star.uno.XComponentContext
+	defaultBootstrap_InitialComponentContext()
+    {
+	return defaultBootstrap_InitialComponentContext(null, null);
+    }
+
+    public static unoidl.com.sun.star.uno.XComponentContext
+	defaultBootstrap_InitialComponentContext(
+	    string iniFile,
+	    IDictionaryEnumerator bootstrapParameters)
+    {
+	if (bootstrapParameters != null)
+	{
+	    bootstrapParameters.Reset();
+	    while (bootstrapParameters.MoveNext())
+		native_bootstrap_set((string)bootstrapParameters.Key,
+				     (string)bootstrapParameters.Value);
+	}
+
+	// bootstrap native uno
+	IntPtr nativeContext;
+	if (iniFile == null)
+	{
+	    nativeContext = native_defaultBootstrap_InitialComponentContext();
+	}
+	else
+	{
+	    nativeContext = native_defaultBootstrap_InitialComponentContext(iniFile);
+	}
+
+	return (unoidl.com.sun.star.uno.XComponentContext)MapToManaged(nativeContext);
+    }
+
+    public static unoidl.com.sun.star.uno.XComponentContext bootstrap()
+    {
+	IntPtr nativeContext = native_bootstrap();
+	return (unoidl.com.sun.star.uno.XComponentContext)MapToManaged(nativeContext);
+    }
+
+    static object MapToManaged(IntPtr cplusplusInterface)
+    {
+	IntPtr unoMapping = native_get_mapping_to_cli();
+	if (unoMapping == IntPtr.Zero)
+	    ;// FIXME throw some exception
+
+	IntPtr managed = native_mapping_map_interface(cplusplusInterface);
+	GCHandle handle = GCHandle.op_Explicit(managed);
+	object ret = handle.Target;
+	handle.Free();
+	return ret;
+    }
+
+    [DllImport("cli_uno")]
+    private static extern void native_bootstrap_set(
+	[MarshalAs(UnmanagedType.LPWStr)] string key,
+	[MarshalAs(UnmanagedType.LPWStr)] string value);
+
+    [DllImport("cli_uno", EntryPoint="native_defaultBootstrap_InitialComponentContext")]
+    private static extern IntPtr native_defaultBootstrap_InitialComponentContext();
+
+    [DllImport("cli_uno", EntryPoint="native_defaultBootstrap_InitialComponentContext_iniFile")]
+    private static extern IntPtr native_defaultBootstrap_InitialComponentContext(
+	[MarshalAs(UnmanagedType.LPWStr)] string iniFile);
+
+    [DllImport("cli_uno")]
+    private static extern IntPtr native_bootstrap();
+
+    [DllImport("cli_uno")]
+    private static extern IntPtr native_get_mapping_to_cli();
+
+    [DllImport("cli_uno")]
+    private static extern IntPtr native_mapping_map_interface(IntPtr cplusplusInterface);
+}
+
+}
