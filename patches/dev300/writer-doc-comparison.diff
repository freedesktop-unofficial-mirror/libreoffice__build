+const Sequence<OUString>& SwCompareConfig::GetPropertyNames()
+{
+    static Sequence<OUString> aNames;
+    if(!aNames.getLength())
+    {
+        const int nCount = 4;
+        aNames.realloc(nCount);
+        static const char* aPropNames[] =
+        {
+            "Mode",							// 0
+            "UseRSID",						// 1
+            "IgnorePieces",				// 2
+            "IgnoreLength"					// 3
+        };
+        OUString* pNames = aNames.getArray();
+        for(int i = 0; i < nCount; i++)
