+inline void lclPushOpCodeMapEntry( ::std::vector< ::com::sun::star::sheet::FormulaOpCodeMapEntry >& rVec, const String* pTable, USHORT nOpCode )
+{
+    ::com::sun::star::sheet::FormulaOpCodeMapEntry aEntry;
