+void ScCompiler::OpCodeMap::putExternal( const String & rSymbol, const String & rAddIn )
+{
+    bool bOk = mpExternalHashMap->insert( ScExternalHashMap::value_type( rSymbol, rAddIn)).second;
+    if (bOk)
