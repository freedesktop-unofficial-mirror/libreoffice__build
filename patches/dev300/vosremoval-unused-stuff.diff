-OThread::TThreadSleep OThread::sleep(const TimeValue& Delay) 
-{
-    TThreadSleep eRet;
-    
-    switch( osl_waitCondition( m_aCondition, &Delay ) )
-    {
-    case osl_cond_result_ok:
-        eRet = TSleep_Normal;
-        break;
-        
-    case osl_cond_result_timeout:
-        eRet = TSleep_Cancel;
-        break;   
-     
-    default:
-        eRet = TSleep_Error;
-        break;
-    }
-    
-    return eRet;
-}
