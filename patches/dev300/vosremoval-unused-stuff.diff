-		for ( i = 0; i < n_Args; ++i )
-			rtl_uString_release( m_aVec[i] );
-
-		delete [] m_aVec;
-
-
-		n_Args = rOther.n_Args;
-		m_aVec = new rtl_uString* [n_Args];
-		for( i = 0; i < n_Args; ++i )
-		{
-			m_aVec[i] = rOther.m_aVec[i];
