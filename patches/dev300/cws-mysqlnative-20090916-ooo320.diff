+        {
+            return null;
+        }
+        buf.append(host);
+        if (n == oooUrl.length())
+        {
+            buf.append('/');
+        }
+        else
+        {
+            loop:
+            while (n < oooUrl.length())
+            {
+                buf.append('/');
+                ++n;
+                int n2 = oooUrl.indexOf('/', n);
+                if (n2 < 0)
+                {
