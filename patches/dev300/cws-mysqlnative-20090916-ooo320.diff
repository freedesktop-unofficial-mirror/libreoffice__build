+/* {{{ OPropertyMap::~OPropertyMap() -I- */
+OPropertyMap::~OPropertyMap()
+{
+	::std::map<sal_Int32 , rtl_uString*>::iterator aIter = m_aPropertyMap.begin();
+	for(; aIter != m_aPropertyMap.end(); ++aIter) {
+		if (aIter->second) {
+			rtl_uString_release(aIter->second);
+		}
+	}
+}
+/* }}} */
