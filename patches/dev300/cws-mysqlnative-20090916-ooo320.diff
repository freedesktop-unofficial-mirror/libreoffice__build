+	OTypeCollection aTypes(	::getCppuType((const  Reference< XMultiPropertySet > *) NULL),
+												::getCppuType((const Reference< XFastPropertySet > *) NULL),
+												::getCppuType((const Reference< XPropertySet > *) NULL));
