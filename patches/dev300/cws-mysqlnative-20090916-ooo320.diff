+				// but SUNPRO 5 compiler (linker) doesn't like this
+				::osl::MutexGuard aGuard(s_aMutex);
+				OSL_ENSURE(s_nRefCount > 0, "OPropertyArrayUsageHelper::~OPropertyArrayUsageHelper : suspicious call : have a refcount of 0 !");
+				if (!--s_nRefCount)
+				{
+					delete s_pProps;
+					s_pProps = NULL;
