-	sal_Int32 nsecs = m_Expired.Nanosec - Now.Nanosec;
-
-	if (nsecs < 0)
-	{
-		if (secs > 0)
-		{
-			secs  -= 1;
-			nsecs += 1000000000L;
-		}
-		else
-			return TTimeValue(0, 0);
-	}
-
-	return TTimeValue(secs, nsecs);
