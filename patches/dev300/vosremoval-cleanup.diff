-{
-	OSL_ASSERT(pNewClass != NULL);
-
-	OClassInfo* pClassRoot = (OClassInfo*)&VOS_CLASSINFO(VOS_NAMESPACE(OObject, vos));
-	
-	pNewClass->m_pNextClass = pClassRoot->m_pNextClass;
-
-	pClassRoot->m_pNextClass = pNewClass;
-}
