-{
-	OSL_ASSERT(pClassName != NULL);
-
-	const OClassInfo* pClass = &VOS_CLASSINFO(VOS_NAMESPACE(OObject, vos));
-	
-	while (pClass != NULL)
-	{
-		if (strcmp(pClassName, pClass->m_pClassName) == 0)
-			break;
-
-		pClass = pClass->m_pNextClass;
-	}
-
-	return (pClass);
-}
-
