+ %.$(STATIC_O): $(srcdir)/%.c
+--- misc/icu/source/config/mh-solaris
++++ misc/build/icu/source/config/mh-solaris
+@@ -18,17 +18,24 @@ THREADSCXXFLAGS += -mt
+ 
