+ 
+ #define SHAPE_MASK 0xF0000000UL
+@@ -174,11 +173,7 @@ void ArabicShaping::shape(const LEUnicode *chars, le_int32 offset, le_int32 char
+         LEUnicode c = chars[in];
+         ShapeType t = getShapeType(c);
+ 
