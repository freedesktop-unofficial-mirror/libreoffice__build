+void
+VbaDocumentBase::Save() throw (uno::RuntimeException)
+{
+    rtl::OUString url = rtl::OUString( RTL_CONSTASCII_USTRINGPARAM(".uno:Save"));
+    uno::Reference< frame::XModel > xModel = getModel();
+    dispatchRequests(xModel,url);
+}
