+    uno::Reference< msforms::XControl > xControlToReturn;
+    if ( xControlShape.is() )
+    {
+        ScVbaControlFactory controlFactory( m_xCtx, xControlShape, xDocOwner );
+        xControlToReturn.set( controlFactory.createControl( xDocOwner ) );
+    }
+    return xControlToReturn;
