[ JDK150 ]
# You need 64bit java, e.g. JDK 1.5.0 for AMD64:
#   http://java.sun.com/j2se/1.5.0/download.jsp
# This JDK needs
#   ln -s motif21/libmawt.so /usr/java/jdk1.5.0_01/jre/lib/amd64/libmawt.so
