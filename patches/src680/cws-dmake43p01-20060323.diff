diff -N dmake/dbug/dbug/factorial.c
--- /dev/null	1 Jan 1970 00:00:00 -0000
+++ dmake/dbug/dbug/factorial.c	26 Sep 2005 00:43:27 -0000	1.1.2.1
@@ -0,0 +1,15 @@
+#include <stdio.h>
+/* User programs should use <local/dbug.h> */
+#include "dbug.h"
+
+int factorial (value)
+     register int value;
+{
+  DBUG_ENTER ("factorial");
+  DBUG_PRINT ("find", ("find %d factorial", value));
+  if (value > 1) {
+    value *= factorial (value - 1);
+  }
+  DBUG_PRINT ("result", ("result is %d", value));
+  DBUG_RETURN (value);
+}
