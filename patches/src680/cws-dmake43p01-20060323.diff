diff -N dmake/dbug/dbug/main.c
--- /dev/null	1 Jan 1970 00:00:00 -0000
+++ dmake/dbug/dbug/main.c	26 Sep 2005 00:43:27 -0000	1.1.2.1
@@ -0,0 +1,27 @@
+#include <stdio.h>
+/* User programs should use <local/dbug.h> */
+#include "dbug.h"
+
+int main (argc, argv)
+     int argc;
+     char *argv[];
+{
+  register int result, ix;
+  extern int factorial (), atoi ();
+
+  DBUG_ENTER ("main");
+  DBUG_PROCESS (argv[0]);
+  for (ix = 1; ix < argc && argv[ix][0] == '-'; ix++) {
+    switch (argv[ix][1]) {
+    case '#':
+      DBUG_PUSH (&(argv[ix][2]));
+      break;
+    }
+  }
+  for (; ix < argc; ix++) {
+    DBUG_PRINT ("args", ("argv[%d] = %s", ix, argv[ix]));
+    result = factorial (atoi (argv[ix]));
+    printf ("%d\n", result);
+  }
+  DBUG_RETURN (0);
+}
