+                 int argc;
+                 char *argv[];
+                 {
+                 #   ifdef DEBUG
+                     printf ("argv[0] = %d\n", argv[0]);
+                 #   endif
+                     /*
+                      *    Rest of program
+                      */
+                 #   ifdef DEBUG
+                     printf ("== done ==\n");
+                 #   endif
+                 }
